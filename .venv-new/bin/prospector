#!/mnt/ssd2/od/brian/lights-n-signs-training/.venv-new/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'prospector==0.12.2','console_scripts','prospector'
__requires__ = 'prospector==0.12.2'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('prospector==0.12.2', 'console_scripts', 'prospector')()
    )
